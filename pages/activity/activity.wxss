/* pages/activity/activity.wxss */
@import '../activityinfo/activityinfo.wxss';
.header{
    height: 60rpx;
    line-height: 60rpx;
    padding: 30rpx;
    background:#f5f5f5;
    position: relative;
    color: #0b9fff;
    font-size: 30rpx;
}
.city{
    padding: 0 20rpx;
    margin-right: 20rpx;
    font-size: 38rpx;
    color: #000;
    border-right: 1px solid #000;
    display: inline-block;
}
.city image{
    width: 32rpx;
    height: 32rpx;
}
