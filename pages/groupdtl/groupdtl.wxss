/* pages/groupdtl/groupdtl.wxss */
@import '../bookstore/bookstore.wxss';
.groupTitle{
    height: 120rpx;
    line-height: 120rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 30rpx;
    border-bottom: 1px solid #e5e5e5;
}
.groupLabel{
    font-size: 38rpx;
}
.delBtn{
    background: #0b9fff;
    border-radius: 8rpx;
    color: #fff;
    font-size: 28rpx;
    display: flex;
    padding: 0 10rpx; 
    align-items: center;
    height: 40%;
}